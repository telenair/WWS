CREATE TABLE t_adresse (address_id INTEGER NOT NULL, ADDRESS VARCHAR(255), ADDRESS2 VARCHAR(255), CITY VARCHAR(255), last_update DATETIME, postal_code VARCHAR(255), PRIMARY KEY (address_id))
CREATE TABLE t_artikelstamm (IDARTIKELNUMMER INTEGER NOT NULL, ANLEGEDATUM DATE, ARTIKELBESCHREIBUNG1 VARCHAR(255), ARTIKELBESCHREIBUNG2 VARCHAR(255), ARTIKELBREITE INTEGER, ARTIKELGEWICHT INTEGER, ARTIKELLAENGE INTEGER, ARTIKELMATCHCODE VARCHAR(255), INVENTURPFLICHTIG VARCHAR(255), LETZTEAENDERUNG DATETIME, UMRECHNUNGSFAKTOR1 DECIMAL(38), UMRECHNUNGSFAKTOR2 DECIMAL(38), UMRECHNUNGSFAKTOR3 DECIMAL(38), UMRECHNUNGSFAKTOR4 DECIMAL(38), UMRECHNUNGSFAKTORME1 VARCHAR(255), UMRECHNUNGSFAKTORME2 VARCHAR(255), UMRECHNUNGSFAKTORME3 VARCHAR(255), UMRECHNUNGSFAKTORME4 VARCHAR(255), PRIMARY KEY (IDARTIKELNUMMER))
CREATE TABLE t_kunde (customer_id INTEGER NOT NULL, CUSTOMERMATCHCODE VARCHAR(255), first_name VARCHAR(255), last_name VARCHAR(255), last_update DATETIME, PRIMARY KEY (customer_id))
